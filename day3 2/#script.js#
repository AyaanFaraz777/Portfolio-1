let posx = 10; 


function drawTile(x, y, width, height){

     
     
}     
}

function setup(){
    canvas = createCanvas(1000, 1000);
    canvas.parent("c");
}

let tileW = 100;
let tileH = tileW;

function draw(){
    for (let i = 0; i < 10; i++){
        for (let j = 0; j < 10; j++){
            drawTile(i*tileW, j*tileH, tileW, tileH);
        }
    }
}